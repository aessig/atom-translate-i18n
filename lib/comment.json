{
    "singlequote": {
        "prefix": {
            "py": "#",
            "js": "mf( 'key', ",
            "html": "{{mf 'key' '",
            "htm": "{{mf 'key' '",
            "php": "/*\n"
        },

        "suffix": {
            "py": "",
            "js": ")",
            "html": "'}}",
            "htm": "'}}",
            "php": "*/\n"
        }

    },

    "doublequote": {
        "prefix": {
            "py": "#",
            "js": "mf( \"key\", ",
            "html": "{{mf \"key\" \"",
            "htm": "{{mf \"key\" \"",
            "php": "//"
        },

        "suffix": {
            "py": "",
            "js": ")",
            "html": "\"}}",
            "htm": "\"}}",
            "php": ""
        }
    },

    "inline": {
        "js": "/**/",
        "html": "<!---->",
        "htm": "<!---->",
        "md": "<!---->",
        "php": "/**/"
    }
}
