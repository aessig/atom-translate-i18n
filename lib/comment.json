{
    "singlequote": {
        "prefix": {
            "py": "#",
            "js": "TAPi18n.__('",
            "html": "{{_ '",
            "htm": "{{_ ''",
            "php": "/*\n"
        },

        "suffix": {
            "py": "",
            "js": "')",
            "html": "'}}",
            "htm": "'}}",
            "php": "*/\n"
        }

    },

    "doublequote": {
        "prefix": {
            "py": "#",
            "js": "TAPi18n.__(\"",
            "html": "{{_ \"",
            "htm": "{{_ \"",
            "php": "//"
        },

        "suffix": {
            "py": "",
            "js": "\")",
            "html": "\"}}",
            "htm": "\"}}",
            "php": ""
        }
    },

    "inline": {
        "js": "/**/",
        "html": "<!---->",
        "htm": "<!---->",
        "md": "<!---->",
        "php": "/**/"
    }
}
